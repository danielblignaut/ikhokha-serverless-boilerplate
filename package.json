{
  "name": "ikhokha-serverless",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.ts",
  "types": "./typings/typings.d.ts",
  "scripts": {
    "build:dev": "npx serverless package",
    "build:prod": "NODE_ENV=production npx serverless package",
    "test": "npm run test:coverage",
    "test:coverage": "npx jest  --coverage",
    "test:watch": "npx jest --verbose=false --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "NODE_ENV=development npx serverless offline --host 0.0.0.0 --port 8000",
    "deploy:dev": "npx serverless deploy --stage dev",
    "tsc": "npx tsc",
    "ts-node": "npx ts-node -r tsconfig-paths/register",
    "format:lint": "npx eslint  '**/*.{js,ts,tsx,jsx}'",
    "format:prettier": "npx prettier \"**/*.+(json|yml|yaml|graphql|gql|sass|scss|less|css)\"",
    "format:write": "npm run format:lint -- --fix && npm run format:prettier -- --write",
    "validate": "npm run format:lint && npm run format:prettier -- --list-different && tsc",
    "precommit": "npm run format:write && npm run validate && npm run test",
    "cli:generate-schema": "npm run ts-node --  ./cli-tools/run-save-remote-schema.ts --path ./generated/schema.graphql --url https://ikhokha.myshopify.com/api/graphql --token d2696157b6d517907338605169b708d3 --token-key X-Shopify-Storefront-Access-Token"
  },
  "dependencies": {
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-server-lambda": "^2.4.8",
    "aws-param-store": "^2.1.0",
    "graphql": "^14.2.1",
    "i": "^0.3.6",
    "minimist": "^1.2.0",
    "node-fetch": "^2.3.0",
    "npm": "^6.9.0",
    "source-map-support": "^0.5.10",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/circular-dependency-plugin": "^5.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.12",
    "@types/minimist": "^1.2.0",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^11.13.8",
    "@types/node-fetch": "^2.3.2",
    "@types/serverless": "^1.18.2",
    "@types/valid-url": "^1.0.2",
    "@types/webpack-node-externals": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-schema": "^1.2.2",
    "apollo-server-testing": "^2.4.8",
    "aws-event-mocks": "git+https://github.com/serverless/aws-event-mocks.git",
    "aws-lambda-test-utils": "^1.3.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-import-node": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "circular-dependency-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-jest": "^22.5.1",
    "file-loader": "^3.0.1",
    "graphql-request": "^1.8.2",
    "husky": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^24.7.1",
    "jest-raw-loader": "^1.0.1",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.3.0",
    "prettier": "1.17.0",
    "raw-loader": "^2.0.0",
    "react-loadable": "^5.5.0",
    "serverless": "^1.40.0",
    "serverless-dotenv-plugin": "^2.0.1",
    "serverless-offline": "^4.9.4",
    "serverless-webpack": "^5.2.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
